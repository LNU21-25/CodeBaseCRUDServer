<% layout('layout') %>
  <h1>Login</h1>
  <form action="/auth/login" method="POST">
    <div>
      <label>Username</label>
      <input type="text" name="username" required>
    </div>
    <div>
      <label>Password</label>
      <input type="password" name="password" required>
    </div>
    <button type="submit">Login</button>
  </form>